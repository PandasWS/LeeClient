    "normal": {
        "globalOffset": "0,0",
        "fontColor": "#000000",
        "shadowMode": "offset",
        "shadowColor": "#000000",
        "shadowAlpha": "12",
        "shadowOffset": "1,1"
    },
    "hover": {
        "globalOffset": "0,0",
        "fontColor": "#000000",
        "shadowMode": "offset",
        "shadowColor": "#000000",
        "shadowAlpha": "12",
        "shadowOffset": "1,1"
    },
    "press": {
        "globalOffset": "0,0",
        "fontColor": "#000000",
        "shadowMode": "offset",
        "shadowColor": "#ffffff",
        "shadowAlpha": "50",
        "shadowOffset": "-1,-1"
    },
    "disabled": {
        "globalOffset": "0,0",
        "fontColor": "#6f6f6f",
        "shadowMode": "offset",
        "shadowColor": "#ffffff",
        "shadowAlpha": "50",
        "shadowOffset": "-1,-1"
